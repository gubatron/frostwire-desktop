msgid "Always Send Immediately"
msgstr "Send alltid umiddelbart"

#: gui/com/limegroup/gnutella/gui/bugs/BugManager.java:403
#: gui/com/limegroup/gnutella/gui/options/panes/BugsPaneItem.java:97
msgid "Always Ask For Review"
msgstr "Spør alltid om revidering"

#: gui/com/limegroup/gnutella/gui/bugs/BugManager.java:404
#: gui/com/limegroup/gnutella/gui/options/panes/BugsPaneItem.java:98
msgid "Always Discard All Errors"
msgstr "Vis aldri feilrapporter"

#: gui/com/limegroup/gnutella/gui/bugs/BugManager.java:502
msgid ""
